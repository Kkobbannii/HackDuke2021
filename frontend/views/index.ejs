    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
</head>

<body style="background-color: rgb(180, 160, 237);">
    <nav style="background-color: #967bb6;" class="navbar navbar-expand-lg bd-navbar navbar-dark fixed-top">
        <a class="navbar-brand" href="/">&nbsp; Find pictures</a>
    </nav>
    <div id="godown"></div>
    <div class="top" style="width: 100%;">
        <table class="table2" style="width: 100%">
            <tbody>
                <tr>
                    <td style=" width: 100%;">
                        <form id="myForm" action="/search" method="GET" class="form-inline">
                            <div class="form-group" style="margin: 2%;">
                                <input type="search" name="search" placeholder="Search Pictures..."
                                    class="btn btn-light" style="width: 60%;display: inline-block;opacity: 40%;"
                                    value="<%= q %>">
                                <input type="submit" value="Search" class="btn btn-outline-primary"
                                    style="width: 30%;display: inline-block; ">
                            </div>
                        </form>
                    </td>


                </tr>
            </tbody>

        </table>
    </div>
    </div>

    <div class="main" style="width: 100%; display: block;">
        <table class="table" style="width:100%; text-align: center;">
            <thead>
                <tr>
                    <th scope="col"></th>
                    <th scope="col"></th>
                    <th scope="col"></th>


                </tr>
            </thead>

            <tbody>
                <% for(var i=0; i<5; i++){ %>
                    <tr>


                        <td>
                            <table class="table" style="width:100%; text-align: center;">
                                <tbody>
                                    <img src="../photos/eva.png">
                                </tbody>
                            </table>
                        </td>
                        <td>
                            <table class="table" style="width:100%; text-align: center;">
                                <tbody>
                                    <img src="../photos/eva.png">
                                </tbody>
                            </table>
                        </td>
                        <td>
                            <table class="table" style="width:100%; text-align: center;">
                                <tbody>
                                    <img src="../photos/eva.png">
                                </tbody>
                            </table>
                        </td>
                    </tr>
                    <% } %>
            </tbody>

        </table>
    </div>

</body>

</html>